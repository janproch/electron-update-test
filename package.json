{
  "name": "electron-update-test-pacjson",
  "version": "0.0.21-beta.1",
  "private": false,
  "dependencies": {
    "electron-log": "^4.4.1",
    "electron-updater": "^6.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janproch/electron-update-test.git"
  },
  "build": {
    "artifactName": "eutest-${version}-${os}_${arch}.${ext}",
    "appId": "com.jenasoft.eutest",
    "productName": "EU TEST",
    "asarUnpack": "**/*.node",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "icon.ico",
      "publish": [
        {
          "provider": "github",
          "owner": "janproch",
          "repo": "electron-update-test"
        }
      ]
    },
    "files": [
      "index.js",
      "updaterChannel.js",
      "icon.png"
    ]
  },
  "homepage": "./",
  "scripts": {
    "start": "cross-env DEVMODE=1 electron .",
    "dist": "electron-builder",
    "rebuild": "electron-builder install-app-deps"
  },
  "main": "index.js",
  "devDependencies": {
    "cross-env": "^6.0.3",
    "electron": "30.0.2",
    "electron-builder": "23.1.0"
  }
}
